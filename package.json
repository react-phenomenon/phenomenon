{
  "name": "@phenomenon/slides",
  "version": "1.0.0",
  "homepage": "http://react-phenomenon.github.io/slides",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "build:local": "PUBLIC_URL='/' react-scripts build",
    "server": "node ./scripts/server.js",
    "render": "node ./scripts/render.js",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "lint": "eslint  --ext .ts,.tsx,.js src/",
    "typecheck": "tsc --noEmit",
    "prettier-check": "prettier --check 'src/**/*.{ts,tsx}' 'scripts/**/*.js'",
    "predeploy": "rm -rf build && yarn build",
    "deploy": "gh-pages -d build"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "browserslist": [
    "> 2%",
    "not dead"
  ],
  "dependencies": {
    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "gsap": "^2.1.3",
    "lodash": "^4.17.11",
    "pretty-ms": "^5.0.0",
    "puppeteer": "^1.17.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-hooks-global-state": "^0.9.0",
    "react-use": "^12.2.2",
    "socket.io": "^2.3.0",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@types/express": "^4.17.0",
    "@types/gsap": "^1.20.2",
    "@types/lodash": "^4.14.141",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/socket.io-client": "^1.4.32",
    "@types/styled-components": "^4.1.15",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-preset-react": "^6.24.1",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": ">=3.0.8",
    "lint-staged": ">=9.4.1",
    "prettier": "^1.17.0",
    "react-scripts": "^3.2.0",
    "typescript": "^3.5.1",
    "webpack": "4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  }
}
