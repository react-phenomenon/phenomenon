{
  "name": "@phenomenon/slides",
  "version": "1.0.0",
  "scripts": {
    "watch": "npm-run-all --parallel watch:*",
    "watch:babel": "yarn run build-lib:babel --watch",
    "watch:ts": "yarn run build-lib:ts --watch",
    "build-lib": "npm-run-all clean build-lib:*",
    "build-lib:babel": "NODE_ENV=production babel src/ --out-dir dist --copy-files --extensions .ts,.tsx",
    "build-lib:ts": "tsc --declaration --outDir dist/ --emitDeclarationOnly --declarationMap",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "gsap": "^2.1.3",
    "lodash": "^4.17.11",
    "puppeteer": "^1.17.0",
    "react": "^16.10.2",
    "react-hooks-global-state": "^0.9.0",
    "react-use": "^13.12.1",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@types/express": "^4.17.0",
    "@types/gsap": "^1.20.2",
    "@types/lodash": "^4.14.149",
    "@types/react-dom": "^16.9.1",
    "@types/socket.io-client": "^1.4.32",
    "babel-preset-react-app": "^9.1.0",
    "typescript": "^3.5.1"
  }
}
